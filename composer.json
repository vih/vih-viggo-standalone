{
    "name": "vih/viggo-calendar-extractor",
    "type": "project",
    "description": "Using kalendersiden.dk this library creates a PDF version of calendar items extracted from Viggo",
    "keywords": [ "viggo", "calendar", "kalendersiden" ],
    "homepage": "http://vih.dk/kalender",
    "support": {
       "issues": "https://github.com/vih/vih-viggo-standalone/issues"
    },
    "authors": [
        {
            "name": "Lars Olesen",
            "email": "lars@intraface.dk"
        }
    ],
    "require": {
        "php": "~8.3",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "slim/slim": "~3.5",
        "guzzlehttp/guzzle": "^7.0",
        "kigkonsult/icalcreator": "2.41.30",
        "twig/twig": "~1.33"
    },
    "autoload": {
        "psr-4": {"Kalendersiden\\": "src/"}
    },
    "config": {
        "audit": {
            "ignore": [
                "PKSA-yhcn-xrg3-68b1",
                "PKSA-2wrf-1xmk-1pky"
            ]
        }
    }
}
